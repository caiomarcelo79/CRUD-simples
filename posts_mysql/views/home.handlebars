<h1>Lista de Posts: </h1>

<a href="/cad"><button>Cadastre um post</button></a>

<hr>
{{#each posts}}
    <small>{{createdAt}}</small>
    <h1>{{titulo}}</h1>
    <p>{{conteudo}}</p>
    <a href="/edit/{{id}}"><button>Editar</button></a>
    <a href="/del/{{id}}"><button>Deletar</button></a>
    <hr>
{{/each}}
